#This function contains blockIDs 70 to 79
execute if entity @s[scores={material.bID=70}] run setblock ~ ~ ~ minecraft:stripped_acacia_wood replace
execute if entity @s[scores={material.bID=71}] run setblock ~ ~ ~ minecraft:stripped_dark_oak_wood replace
execute if entity @s[scores={material.bID=72}] run setblock ~ ~ ~ minecraft:oak_leaves[persistent=true] replace
execute if entity @s[scores={material.bID=73}] run setblock ~ ~ ~ minecraft:spruce_leaves[persistent=true] replace
execute if entity @s[scores={material.bID=74}] run setblock ~ ~ ~ minecraft:birch_leaves[persistent=true] replace
execute if entity @s[scores={material.bID=75}] run setblock ~ ~ ~ minecraft:jungle_leaves[persistent=true] replace
execute if entity @s[scores={material.bID=76}] run setblock ~ ~ ~ minecraft:acacia_leaves[persistent=true] replace
execute if entity @s[scores={material.bID=77}] run setblock ~ ~ ~ minecraft:dark_oak_leaves[persistent=true] replace
