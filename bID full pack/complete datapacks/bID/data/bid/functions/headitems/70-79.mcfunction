#This function contains blockIDs 70 to 79
execute if entity @s[scores={material.bID=70}] run replaceitem entity @s armor.head minecraft:stripped_acacia_wood 1

execute if entity @s[scores={material.bID=71}] run replaceitem entity @s armor.head minecraft:stripped_dark_oak_wood 1

execute if entity @s[scores={material.bID=78}] run replaceitem entity @s armor.head minecraft:oak_leaves[persistent=true] 1

execute if entity @s[scores={material.bID=79}] run replaceitem entity @s armor.head minecraft:spruce_leaves[persistent=true] 1

