#This function contains blockIDs 70 to 79
execute if entity @s[scores={material.bID=70}] store result score @s detected.bID run clear @s minecraft:stripped_acacia_wood 0

execute if entity @s[scores={material.bID=71}] store result score @s detected.bID run clear @s minecraft:stripped_dark_oak_wood 0

execute if entity @s[scores={material.bID=78}] store result score @s detected.bID run clear @s minecraft:oak_leaves[persistent=true] 0

execute if entity @s[scores={material.bID=79}] store result score @s detected.bID run clear @s minecraft:spruce_leaves[persistent=true] 0

